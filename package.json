{
  "name": "ev-queue-manager",
  "version": "1.0.0",
  "description": "A simple WhatsApp group queue manager bot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest ./tests --coverage",
    "lint": "eslint .",
    "format": "prettier --write \"./**/*.+(js|mjs|ts|json)\"",
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "check-updates": "npx npm-check-updates -u && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaaufauvre/ev-queue-manager.git"
  },
  "author": "Jean-Alexis Aufauvre",
  "bugs": {
    "url": "https://github.com/jaaufauvre/ev-queue-manager/issues"
  },
  "homepage": "https://github.com/jaaufauvre/ev-queue-manager#readme",
  "dependencies": {
    "@whiskeysockets/baileys": "7.0.0-rc.9",
    "node-cron": "4.2.1",
    "qrcode-terminal": "0.12.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@types/jest": "30.0.0",
    "@types/qrcode-terminal": "0.12.2",
    "@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "8.51.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "jest": "30.2.0",
    "nodemon": "3.1.11",
    "prettier": "3.7.4",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  }
}
