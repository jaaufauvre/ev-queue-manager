{
  "name": "ev-queue-manager",
  "version": "1.0.0",
  "description": "A simple WhatsApp group queue manager bot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest ./tests --coverage",
    "lint": "eslint .",
    "format": "prettier --write \"./**/*.+(js|mjs|ts|json)\"",
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "check-updates": "npx npm-check-updates -u && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaaufauvre/ev-queue-manager.git"
  },
  "author": "Jean-Alexis Aufauvre",
  "bugs": {
    "url": "https://github.com/jaaufauvre/ev-queue-manager/issues"
  },
  "homepage": "https://github.com/jaaufauvre/ev-queue-manager#readme",
  "dependencies": {
    "@whiskeysockets/baileys": "6.17.16",
    "node-cron": "4.0.5",
    "qrcode-terminal": "0.12.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.27.0",
    "@types/jest": "29.5.14",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.5",
    "jest": "29.7.0",
    "nodemon": "3.1.10",
    "prettier": "3.5.3",
    "ts-jest": "29.3.4",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  }
}
